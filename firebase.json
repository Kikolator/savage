{
  "hosting": [
    {
      "target": "api",
      "public": "frontend/api",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "function": {
            "functionId": "api",
            "region": "europe-west1",
            "pinTag": true
          }
        }
      ]
    },
    {
      "target": "referral",
      "public": "frontend/referral/build/web",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "qr",
      "public": "frontend/qr",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "redirects": [
        {
          "source": "/",
          "destination": "https://savage-coworking.com/?utm_source=qr&utm_medium=qr-no-code&utm_campaign=qr",
          "type": 301
        },
